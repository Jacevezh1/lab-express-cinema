



<h2> IRONHACK MOVIES </h2>
<ul>
    {{#each data}}
    <div class="card" style="width: 18rem;">
        <img src={{this.image}} class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title"> {{this.image}} </h5>
            <p class="card-text"> {{this._id}} </p>
            <a href="/movies" class="btn btn-primary">Details</a>
        </div>  
    <div>
    {{/each}}
</ul>

